.table_top {
    border: 1px solid #e4e4ee;
    text-align: center;
    overflow: hidden;
}

.table_top>view {
    background: #f8f8ff;
    color: #797979;
    height: 110rpx;
    line-height: 110rpx;
    font-size: 28rpx;
}

.table_top>view:nth-child(1) {
    border-right: 1px solid #e4e4ee;
    width: calc(20% - 1px);
}

.table_top>view:nth-child(2) {
    border-right: 1px solid #e4e4ee;
    width: calc(40% - 1px);
}

.wid20 {
    float: left;
    width: 20%;
}

.wid40 {
    float: left;
    width: 40%;
}

.table_content {
    overflow: hidden;
    clear: both;
    text-align: center;
    line-height: 110rpx;
    height: 110rpx;
}

.table_content>view:nth-child(1) {
    border-right: 1px solid #e4e4ee;
    width: calc(20% - 1px);
}

.table_content>view:nth-child(2) {
    border-right: 1px solid #e4e4ee;
    width: calc(40% - 1px);
    font-size: 28rpx;
}

.table_content {
    border-top: 1px solid #e4e4ee;
    border-left: 1px solid #e4e4ee;
    border-right: 1px solid #e4e4ee;
}

.table_content:last-child {
    border-bottom: 1px solid #e4e4ee;
}

checkbox .wx-checkbox-input {
    width: 34rpx;
    height: 34rpx;
    border-radius: 50%;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #e94638;
    border-color: #e94638;
}

/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 20rpx;
    height: 20rpx;
    line-height: 20rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%, -50%) scale(1);
    -webkit-transform: translate(-50%, -50%) scale(1);
}

