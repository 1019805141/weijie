/* pages/my/acceptance/acceptance.wxss */
.title {
    margin: 30rpx 0;
}

.title>i {
    background: #e94638;
    border-bottom-right-radius: 50px;
    border-bottom-left-radius: 50px;
    border-top-right-radius: 50px;
    border-top-left-radius: 50px;
    width: 12rpx;
    height: 34rpx;
    display: block;
    float: left;
    margin-top: 6rpx;
