/* pages/index/commander/commander.wxss */
page{
    background: #f6f6f6;
}
.top{
    background: #ffffff;
    padding: 30rpx 0;
    overflow: hidden;
}
.top_1{
    font-size: 28rpx;
    color: #878787;
    margin-bottom: 10rpx;
}
.top_left{
    font-size: 32rpx;
    color: #333333;
    float: left;
    width: 88%;
}
.top_right{
    color: #5b5b5b;
    font-size: 26rpx;
    float: right;
}
.list{
    border-radius: 10rpx;
    background: #ffffff;
    padding: 30rpx 20rpx;
    overflow: hidden;
    margin-top: 20rpx;
    box-shadow: 0px 2px 10px #f1f1f1;
}
.list>image{
    width: 98rpx;
    height: 98rpx;
    float: left;
    margin-right: 20rpx;
    border-radius: 50%;
}
.list_text>view{
    font-size: 30rpx;
    display: inline-block;
    margin-right: 10rpx;
}
.list_text>p{
    color: #e94638;
    font-size: 28rpx;
}
.list_text>p>image{
    width: 19rpx;
    height: 24rpx;

}
.list_text{
    float: left;
    width: 60%;
}
.list_text>span{
    font-size: 26rpx;
    display: block;
    color: #666666;
    margin-top: 10rpx;
}
.list_right{
    float: right;
    background: #e94638;
    border-radius: 100rpx;
    color: #ffffff;
    text-align: center;
    font-size: 30rpx;
    padding: 6rpx 26rpx;
    margin-top: 20rpx;
}
.zanwu{
    margin: 50rpx 0;
  }
  .zanwu image{
    width: 305rpx;
    height:331rpx;
    display: block;
    margin: 0 auto;
    margin-top: 300rpx;
  }
  .zanwu text{
    text-align: center;
    margin-top: 30rpx;
    display: block;
    font-size: 28rpx;
    color: #a6a6a9;
  }