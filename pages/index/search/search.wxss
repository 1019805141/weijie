/* pages/index/search/search.wxss */
page{
    background: #f6f6f6;
}
.index_top_search {
    position: relative;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
  }
  
  .index_top_search>input {
    height: 60rpx;
    line-height: 60rpx;
    background: #eeeeee;
    border-radius: 100rpx;
    padding-left: 30rpx;
    font-size: 28rpx;
  }
  
  .index_top_search image {
    width: 26rpx;
    height: 26rpx;
    position: absolute;
    left: 40%;
    top: 26%;
  
  }
  .bg{
      background: #ffffff;
      overflow: hidden;
  }

  .index_cp{
    border-bottom: 1px solid #e4e4e4;
    padding-bottom: 30rpx;
    overflow: hidden;
    margin-bottom: 20rpx;
  }
  .index_cp>image{
    width: 250rpx;
    height: 250rpx;
    border-radius: 20rpx;
    float: left;
  }
  .index_cp_right{
    float: left;
    margin-left: 20rpx;
    width: 418rpx;
  }
  .index_cp_right_title{
    color: #333333;
    font-size: 32rpx;
    margin-bottom: 10rpx;
  }
  .index_cp_right>p{
    font-size: 28rpx;
    display: block;
    color: #7a7a7a;
    margin-bottom: 10rpx;
  }
  .index_cp_right_bq{
    color: #e94638;
    border: 1px solid #e94638;
    border-radius: 50rpx;
    display: inline-block;
    font-size: 26rpx;
    padding: 4rpx 18rpx;
  }
  .index_cp_fenshu{
    color: #9f9f9f;
    font-size: 28rpx;
    margin-top: 10rpx;
  }
  .index_cp_fenshu>span{
    color: #e94638;
  }
  .index_cp_jiage{
    overflow: hidden;
    margin-top: 10rpx;
  }
  .index_cp_jiage>p{
    color: #e94638;
    float: left;
    display: block;
  }
  .index_cp_jiage>p>span{
    font-weight: bold;
  }
  .index_cp:last-child{
      border-bottom: none;
  }
  .title{
    margin: 30rpx 0;
  }
  .title>i{
      background: #e94638;
      border-bottom-right-radius:50px;
      border-bottom-left-radius:50px;
      border-top-right-radius:50px;
      border-top-left-radius:50px;
      width: 12rpx;
      height: 34rpx;
      display: block;
      float: left;
      margin-top: 6rpx;
      margin-right: 10rpx;
  }
  .title>view{
      font-size: 32rpx;
  }
  .row{
      margin-left: -15rpx;
      margin-right: -15rpx;
      overflow: hidden;
  }
  .list_box{
    background: #ffffff;
    border-radius: 10rpx;
    box-shadow: 0px 2px 10px #f1f1f1;
    float: left;
    width:calc(50% - 30rpx);
    margin: 15rpx;
}
.list_box>image{
    width: 320rpx;
    height: 260rpx;
    margin: 0 auto;
    margin-bottom: 20rpx;
    display: block;
    border-radius:10rpx 10rpx 0 0;
}
.list_bq{
  white-space:nowrap;
text-overflow:ellipsis;
-webkit-text-overflow:ellipsis;
overflow:hidden;
}
.list_bq>p{
  background: #f06e4b;
  text-align: center;
  color: #ffffff;
  display: inline-block;
  font-size: 24rpx;
  margin-left: 20rpx;
  padding: 2rpx 10rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
}
.list_box{
  color: #333333;
  font-size: 28rpx;
  padding-bottom: 20rpx;
}
.list_ms{
  color: #777777;
  font-size: 26rpx;
  display: block;
  margin-top: 14rpx;
  margin-left: 20rpx;
  margin-bottom: 30rpx;
  white-space:nowrap;
text-overflow:ellipsis;
-webkit-text-overflow:ellipsis;
overflow:hidden;
}
.list_jine{
  overflow: hidden;
}
.list_jine>p{
  color: #e94638;
  float: left;
  font-size: 28rpx;
  margin-left: 20rpx;
}
.list_jine>p>span{
  font-size: 32rpx;
}
.list_jine>view{
  background: #e94638;
      color: #ffffff;
      text-align: center;
      float: right;
      font-size: 28rpx;
      border-radius: 50rpx;
      padding: 6rpx 18rpx;
      margin-right: 20rpx;
}
.zanwu{
  margin: 50rpx 0;
}
.zanwu image{
  width: 305rpx;
  height:331rpx;
  display: block;
  margin: 0 auto;
}
.zanwu text{
  text-align: center;
  margin-top: 30rpx;
  display: block;
  font-size: 28rpx;
  color: #a6a6a9;
}



.index_cp_jiage>view{
  float: right;
}

/*主容器*/

.stepper { 
  width: 86px; 
  height: 26px; 
  /*给主容器设一个边框*/
  border-radius: 3px; 
  margin:0 auto; 
} 

  

/*加号和减号*/

.stepper text { 
  text-align: center; 
  float: left; 
} 

  

/*数值*/

.stepper input { 

  width: 40px; 

  height: 26px; 

  float: left; 

  margin: 0 auto; 

  text-align: center; 

  font-size: 12px; 

  /*给中间的input设置左右边框即可*/

  border-left: 1px solid #ccc; 

  border-right: 1px solid #ccc; 

} 
.jia{
  background: #e94638;
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  color: #ffffff;
  line-height: 40rpx;
  text-align: center;
  position:relative;
}
.jia1{
  position: absolute;
  right:30rpx;
}
/*普通样式*/
.stepper .disabled{ 
  color: #777777; 
  background: #dfdfdf;
}
.stepper view{
  float: right;
  margin: 0 10rpx;
  margin-right: 58rpx;
}
.jia2{
  float: left;
  margin-left: 20rpx;
  color: #777777; 
  background: #dfdfdf;
}