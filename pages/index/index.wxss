/*主容器*/

.stepper { 
  width: 80px; 
  height: 26px; 
  /*给主容器设一个边框*/
  border-radius: 3px; 
  margin:0 auto; 
} 

  

/*加号和减号*/

.stepper text { 
  text-align: center; 
  float: left; 
} 

  

/*数值*/

.stepper input { 

  width: 40px; 

  height: 26px; 

  float: left; 

  margin: 0 auto; 

  text-align: center; 

  font-size: 12px; 

  /*给中间的input设置左右边框即可*/

  border-left: 1px solid #ccc; 

  border-right: 1px solid #ccc; 

} 
